<h1>Log in</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:session, url: login_path) do |f| %>
    <div>
      <%= f.label :account %>
      <%= f.email_field :account, class: 'form-control' %>
    </div>
    <div>
      <%= f.label :password %>
      <%= f.password_field :password, class: 'form-control' %>
    </div>
      <%= f.submit "Log in", class: "btn btn-primary" %>
    <% end %>

    <%=display_whohpq_link?()%>
  </div>
</div>
